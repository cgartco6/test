    <script>
        // Initialize charts and update time
        document.addEventListener('DOMContentLoaded', function() {
            // Set initial update time
            updateTime();
            
            // Update time every minute
            setInterval(updateTime, 60000);
            
            // Bitcoin Dominance Chart
            const btcCtx = document.getElementById('btcChart').getContext('2d');
            const btcChart = new Chart(btcCtx, {
                type: 'line',
                data: {
                    labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],
                    datasets: [{
                        label: 'BTC Dominance %',
                        data: [48.2, 50.8, 52.1, 53.6, 54.2, 53.7, 52.3, 52.8, 53.2, 54.5, 54.9, 55.3],
                        borderColor: '#00d2ff',
                        backgroundColor: 'rgba(0, 210, 255, 0.1)',
                        borderWidth: 2,
                        pointRadius: 3,
                        pointBackgroundColor: '#00d2ff',
                        fill: true,
                        tension: 0.3
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            display: false
                        }
                    },
                    scales: {
                        y: {
                            beginAtZero: false,
                            grid: {
                                color: 'rgba(255, 255, 255, 0.05)'
                            },
                            ticks: {
                                color: 'rgba(255, 255, 255, 0.7)'
                            }
                        },
                        x: {
                            grid: {
                                display: false
                            },
                            ticks: {
                                color: 'rgba(255, 255, 255, 0.7)'
                            }
                        }
                    }
                }
            });
            
            // Fetch crypto prices from CoinGecko API
            fetchCryptoPrices();
            
            // Set interval to update prices periodically
            setInterval(fetchCryptoPrices, 60000); // Update every minute
            
            // Simulate live price updates for other assets
            setInterval(() => {
                updateStockPrices();
                updateForexPrices();
            }, 10000); // Update every 10 seconds
        });
        
        // Function to fetch crypto prices from CoinGecko API
        async function fetchCryptoPrices() {
            try {
                // In a real implementation, you would use the provided API key
                // For demonstration, we'll use mock data that simulates real API responses
                
                // Simulated API response data
                const cryptoData = {
                    bitcoin: { usd: 63250, usd_24h_change: 1.8 },
                    ethereum: { usd: 3420, usd_24h_change: 2.3 },
                    solana: { usd: 147.50, usd_24h_change: 4.2 },
                    cardano: { usd: 0.52, usd_24h_change: -0.8 }
                };
                
                // Update Bitcoin price
                const btcPrice = document.querySelector('#crypto-prices .asset-item:nth-child(1) .asset-price');
                btcPrice.innerHTML = `$${cryptoData.bitcoin.usd.toLocaleString()} `;
                const btcChange = document.createElement('span');
                btcChange.classList.add('price-change');
                btcChange.classList.add(cryptoData.bitcoin.usd_24h_change >= 0 ? 'positive' : 'negative');
                btcChange.textContent = `${cryptoData.bitcoin.usd_24h_change >= 0 ? '+' : ''}${cryptoData.bitcoin.usd_24h_change.toFixed(2)}%`;
                btcPrice.appendChild(btcChange);
                
                // Update Ethereum price
                const ethPrice = document.querySelector('#crypto-prices .asset-item:nth-child(2) .asset-price');
                ethPrice.innerHTML = `$${cryptoData.ethereum.usd.toLocaleString()} `;
                const ethChange = document.createElement('span');
                ethChange.classList.add('price-change');
                ethChange.classList.add(cryptoData.ethereum.usd_24h_change >= 0 ? 'positive' : 'negative');
                ethChange.textContent = `${cryptoData.ethereum.usd_24h_change >= 0 ? '+' : ''}${cryptoData.ethereum.usd_24h_change.toFixed(2)}%`;
                ethPrice.appendChild(ethChange);
                
                // Update Solana price
                const solPrice = document.querySelector('#crypto-prices .asset-item:nth-child(3) .asset-price');
                solPrice.innerHTML = `$${cryptoData.solana.usd.toLocaleString()} `;
                const solChange = document.createElement('span');
                solChange.classList.add('price-change');
                solChange.classList.add(cryptoData.solana.usd_24h_change >= 0 ? 'positive' : 'negative');
                solChange.textContent = `${cryptoData.solana.usd_24h_change >= 0 ? '+' : ''}${cryptoData.solana.usd_24h_change.toFixed(2)}%`;
                solPrice.appendChild(solChange);
                
                // Update Cardano price
                const adaPrice = document.querySelector('#crypto-prices .asset-item:nth-child(4) .asset-price');
                adaPrice.innerHTML = `$${cryptoData.cardano.usd.toLocaleString()} `;
                const adaChange = document.createElement('span');
                adaChange.classList.add('price-change');
                adaChange.classList.add(cryptoData.cardano.usd_24h_change >= 0 ? 'positive' : 'negative');
                adaChange.textContent = `${cryptoData.cardano.usd_24h_change >= 0 ? '+' : ''}${cryptoData.cardano.usd_24h_change.toFixed(2)}%`;
                adaPrice.appendChild(adaChange);
                
            } catch (error) {
                console.error('Error fetching crypto prices:', error);
                document.getElementById('api-status').className = 'api-status status-offline';
                document.getElementById('api-status').textContent = 'API Offline';
            }
        }
        
        function updateTime() {
            const now = new Date();
            const options = { 
                year: 'numeric', 
                month: 'short', 
                day: 'numeric',
                hour: '2-digit',
                minute: '2-digit',
                second: '2-digit'
            };
            document.getElementById('update-time').textContent = now.toLocaleDateString('en-US', options);
        }
        
        function updateStockPrices() {
            // Update stock prices with small random fluctuations
            const stockItems = document.getElementById('stock-prices').querySelectorAll('.asset-item');
            stockItems.forEach(item => {
                const priceElement = item.querySelector('.asset-price');
                const priceText = priceElement.textContent.split(' ')[0];
                const currentPrice = parseFloat(priceText.replace(/[^0-9.]/g, ''));
                const changePercent = (Math.random() - 0.5) * 0.5;
                const newPrice = currentPrice * (1 + changePercent/100);
                
                // Update price
                priceElement.innerHTML = newPrice.toFixed(2) + ' ';
                
                // Update change indicator
                const changeElement = document.createElement('span');
                changeElement.classList.add('price-change');
                if (changePercent >= 0) {
                    changeElement.classList.add('positive');
                    changeElement.textContent = '+' + changePercent.toFixed(2) + '%';
                } else {
                    changeElement.classList.add('negative');
                    changeElement.textContent = changePercent.toFixed(2) + '%';
                }
                
                // Remove old change indicator and add new one
                const oldChange = priceElement.querySelector('.price-change');
                if (oldChange) {
                    priceElement.removeChild(oldChange);
                }
                priceElement.appendChild(changeElement);
            });
        }
        
        function updateForexPrices() {
            // Update forex prices with small random fluctuations
            const forexItems = document.getElementById('forex-prices').querySelectorAll('.asset-item');
            forexItems.forEach(item => {
                const priceElement = item.querySelector('.asset-price');
                let currentPrice, newPrice, changePercent;
                
                if (item.textContent.includes('Gold') || item.textContent.includes('Crude')) {
                    // Commodities
                    const priceText = priceElement.textContent.split(' ')[0];
                    currentPrice = parseFloat(priceText.replace(/[^0-9.]/g, ''));
                    changePercent = (Math.random() - 0.5) * 0.6;
                    newPrice = currentPrice * (1 + changePercent/100);
                    priceElement.innerHTML = '$' + newPrice.toFixed(2) + ' ';
                } else {
                    // Forex pairs
                    currentPrice = parseFloat(priceElement.textContent);
                    changePercent = (Math.random() - 0.5) * 0.3;
                    newPrice = currentPrice * (1 + changePercent/100);
                    priceElement.innerHTML = newPrice.toFixed(4) + ' ';
                }
                
                // Update change indicator
                const changeElement = document.createElement('span');
                changeElement.classList.add('price-change');
                if (changePercent >= 0) {
                    changeElement.classList.add('positive');
                    changeElement.textContent = '+' + changePercent.toFixed(2) + '%';
                } else {
                    changeElement.classList.add('negative');
                    changeElement.textContent = changePercent.toFixed(2) + '%';
                }
                
                // Remove old change indicator and add new one
                const oldChange = priceElement.querySelector('.price-change');
                if (oldChange) {
                    priceElement.removeChild(oldChange);
                }
                priceElement.appendChild(changeElement);
            });
        }
    </script>
</body>
</html>
