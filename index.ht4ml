    <script>
        // Initialize charts and update time
        document.addEventListener('DOMContentLoaded', function() {
            // Set initial update time
            updateTime();
            
            // Update time every minute
            setInterval(updateTime, 60000);
            
            // Bitcoin Dominance Chart
            const btcCtx = document.getElementById('btcChart').getContext('2d');
            const btcChart = new Chart(btcCtx, {
                type: 'line',
                data: {
                    labels: ['6am', '9am', '12pm', '3pm', '6pm', '9pm', '12am', '3am', '6am'],
                    datasets: [{
                        label: 'BTC Dominance %',
                        data: [53.8, 53.5, 53.9, 54.2, 54.5, 54.3, 54.1, 54.0, 54.2],
                        borderColor: '#00d2ff',
                        backgroundColor: 'rgba(0, 210, 255, 0.1)',
                        borderWidth: 2,
                        pointRadius: 3,
                        pointBackgroundColor: '#00d2ff',
                        fill: true,
                        tension: 0.3
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            display: false
                        }
                    },
                    scales: {
                        y: {
                            beginAtZero: false,
                            grid: {
                                color: 'rgba(255, 255, 255, 0.05)'
                            },
                            ticks: {
                                color: 'rgba(255, 255, 255, 0.7)'
                            }
                        },
                        x: {
                            grid: {
                                display: false
                            },
                            ticks: {
                                color: 'rgba(255, 255, 255, 0.7)'
                            }
                        }
                    }
                }
            });
            
            // Simulate live price updates
            setInterval(() => {
                updateBitcoinDominance();
                updateCryptoPrices();
                updateAltcoinPrices();
                updateForexPrices();
            }, 10000); // Update every 10 seconds
            
            // Initial price update
            updateBitcoinDominance();
            updateCryptoPrices();
            updateAltcoinPrices();
            updateForexPrices();
        });
        
        function updateTime() {
            const now = new Date();
            const options = { 
                year: 'numeric', 
                month: 'short', 
                day: 'numeric',
                hour: '2-digit',
                minute: '2-digit',
                second: '2-digit'
            };
            document.getElementById('update-time').textContent = now.toLocaleDateString('en-US', options);
        }
        
        function updateBitcoinDominance() {
            // Update Bitcoin dominance with small random fluctuations
            const dominanceElement = document.getElementById('btc-dominance');
            const changeElement = document.getElementById('btc-dominance-change');
            
            const currentDominance = parseFloat(dominanceElement.textContent);
            const changePercent = (Math.random() - 0.45) * 0.3; // Slightly biased to positive
            const newDominance = currentDominance + changePercent;
            
            // Update dominance
            dominanceElement.textContent = newDominance.toFixed(1) + '%';
            
            // Update change indicator
            if (changePercent >= 0) {
                changeElement.className = 'price-change positive';
                changeElement.textContent = '+' + changePercent.toFixed(1) + '%';
            } else {
                changeElement.className = 'price-change negative';
                changeElement.textContent = changePercent.toFixed(1) + '%';
            }
        }
        
        function updateCryptoPrices() {
            // Update crypto prices with small random fluctuations
            const cryptoItems = document.getElementById('crypto-prices').querySelectorAll('.asset-item');
            cryptoItems.forEach(item => {
                const priceElement = item.querySelector('.asset-price');
                const priceText = priceElement.textContent.split(' ')[0];
                const currentPrice = parseFloat(priceText.replace(/[^0-9.]/g, ''));
                const changePercent = (Math.random() - 0.4) * 2; // Slightly biased to positive
                const newPrice = currentPrice * (1 + changePercent/100);
                
                // Update price
                if (item.textContent.includes('BTC')) {
                    priceElement.innerHTML = '$' + newPrice.toLocaleString(undefined, {maximumFractionDigits: 0}) + ' ';
                } else if (item.textContent.includes('ETH')) {
                    priceElement.innerHTML = '$' + newPrice.toLocaleString(undefined, {maximumFractionDigits: 0}) + ' ';
                } else if (item.textContent.includes('SOL')) {
                    priceElement.innerHTML = '$' + newPrice.toLocaleString(undefined, {maximumFractionDigits: 0}) + ' ';
                } else {
                    priceElement.innerHTML = '$' + newPrice.toFixed(2) + ' ';
                }
                
                // Update change indicator
                const changeElement = document.createElement('span');
                changeElement.classList.add('price-change');
                if (changePercent >= 0) {
                    changeElement.classList.add('positive');
                    changeElement.textContent = '+' + changePercent.toFixed(2) + '%';
                } else {
                    changeElement.classList.add('negative');
                    changeElement.textContent = changePercent.toFixed(2) + '%';
                }
                
                // Remove old change indicator and add new one
                const oldChange = priceElement.querySelector('.price-change');
                if (oldChange) {
                    priceElement.removeChild(oldChange);
                }
                priceElement.appendChild(changeElement);
            });
        }
        
        function updateAltcoinPrices() {
            // Update altcoin prices with small random fluctuations (more volatile)
            const altcoinItems = document.getElementById('altcoin-prices').querySelectorAll('.asset-item');
            altcoinItems.forEach(item => {
                const priceElement = item.querySelector('.asset-price');
                const priceText = priceElement.textContent.split(' ')[0];
                const currentPrice = parseFloat(priceText.replace(/[^0-9.]/g, ''));
                const changePercent = (Math.random() - 0.3) * 4; // More volatile, biased to positive
                const newPrice = currentPrice * (1 + changePercent/100);
                
                // Update price
                priceElement.innerHTML = '$' + newPrice.toFixed(2) + ' ';
                
                // Update change indicator
                const changeElement = document.createElement('span');
                changeElement.classList.add('price-change');
                if (changePercent >= 0) {
                    changeElement.classList.add('positive');
                    changeElement.textContent = '+' + changePercent.toFixed(2) + '%';
                } else {
                    changeElement.classList.add('negative');
                    changeElement.textContent = changePercent.toFixed(2) + '%';
                }
                
                // Remove old change indicator and add new one
                const oldChange = priceElement.querySelector('.price-change');
                if (oldChange) {
                    priceElement.removeChild(oldChange);
                }
                priceElement.appendChild(changeElement);
            });
        }
        
        function updateForexPrices() {
            // Update forex prices with small random fluctuations
            const forexItems = document.getElementById('forex-prices').querySelectorAll('.asset-item');
            forexItems.forEach(item => {
                const priceElement = item.querySelector('.asset-price');
                let currentPrice, newPrice, changePercent;
                
                if (item.textContent.includes('Gold') || item.textContent.includes('Crude')) {
                    // Commodities
                    const priceText = priceElement.textContent.split(' ')[0];
                    currentPrice = parseFloat(priceText.replace(/[^0-9.]/g, ''));
                    changePercent = (Math.random() - 0.5) * 0.6;
                    newPrice = currentPrice * (1 + changePercent/100);
                    priceElement.innerHTML = '$' + newPrice.toFixed(2) + ' ';
                } else {
                    // Forex pairs
                    currentPrice = parseFloat(priceElement.textContent);
                    changePercent = (Math.random() - 0.5) * 0.3;
                    newPrice = currentPrice * (1 + changePercent/100);
                    priceElement.innerHTML = newPrice.toFixed(4) + ' ';
                }
                
                // Update change indicator
                const changeElement = document.createElement('span');
                changeElement.classList.add('price-change');
                if (changePercent >= 0) {
                    changeElement.classList.add('positive');
                    changeElement.textContent = '+' + changePercent.toFixed(2) + '%';
                } else {
                    changeElement.classList.add('negative');
                    changeElement.textContent = changePercent.toFixed(2) + '%';
                }
                
                // Remove old change indicator and add new one
                const oldChange = priceElement.querySelector('.price-change');
                if (oldChange) {
                    priceElement.removeChild(oldChange);
                }
                priceElement.appendChild(changeElement);
            });
        }
    </script>
</body>
