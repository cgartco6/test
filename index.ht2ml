<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SynthAI Financial Assistant</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700&family=Roboto:wght@300;400;500&display=swap" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        body {
            font-family: 'Roboto', sans-serif;
            background: linear-gradient(135deg, #0f2027, #203a43, #2c5364);
            color: #fff;
            line-height: 1.6;
            min-height: 100vh;
            padding: 20px;
        }
        
        .container {
            max-width: 1400px;
            margin: 0 auto;
        }
        
        header {
            text-align: center;
            margin-bottom: 30px;
            padding: 20px;
            background: rgba(255, 255, 255, 0.05);
            border-radius: 15px;
            backdrop-filter: blur(10px);
            box-shadow: 0 8px 32px rgba(0, 0, 0, 0.2);
        }
        
        h1 {
            font-family: 'Montserrat', sans-serif;
            font-weight: 700;
            font-size: 2.8rem;
            margin-bottom: 10px;
            background: linear-gradient(90deg, #00d2ff, #3a7bd5);
            -webkit-background-clip: text;
            background-clip: text;
            -webkit-text-fill-color: transparent;
        }
        
        .subtitle {
            font-size: 1.2rem;
            opacity: 0.8;
            margin-bottom: 20px;
        }
        
        .dashboard {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 25px;
            margin-bottom: 30px;
        }
        
        .card {
            background: rgba(255, 255, 255, 0.05);
            border-radius: 15px;
            padding: 25px;
            backdrop-filter: blur(10px);
            box-shadow: 0 8px 32px rgba(0, 0, 0, 0.2);
            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }
        
        .card:hover {
            transform: translateY(-5px);
            box-shadow: 0 12px 40px rgba(0, 0, 0, 0.3);
        }
        
        .card-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 20px;
            padding-bottom: 15px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.1);
        }
        
        .card-title {
            font-family: 'Montserrat', sans-serif;
            font-weight: 600;
            font-size: 1.4rem;
        }
        
        .card-icon {
            font-size: 1.8rem;
            opacity: 0.7;
        }
        
        .price {
            font-size: 2.2rem;
            font-weight: 700;
            margin: 15px 0;
            color: #00d2ff;
        }
        
        .price-change {
            display: inline-block;
            padding: 5px 12px;
            border-radius: 20px;
            font-weight: 500;
            margin-left: 10px;
            font-size: 0.9rem;
        }
        
        .positive {
            background: rgba(76, 175, 80, 0.2);
            color: #4CAF50;
        }
        
        .negative {
            background: rgba(244, 67, 54, 0.2);
            color: #F44336;
        }
        
        .asset-list {
            list-style: none;
        }
        
        .asset-item {
            display: flex;
            justify-content: space-between;
            padding: 15px 0;
            border-bottom: 1px solid rgba(255, 255, 255, 0.1);
        }
        
        .asset-name {
            display: flex;
            align-items: center;
            gap: 10px;
        }
        
        .asset-icon {
            width: 30px;
            height: 30px;
            background: rgba(255, 255, 255, 0.1);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
        }
        
        .prediction-card {
            grid-column: span 2;
        }
        
        .prediction-list {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 20px;
        }
        
        .prediction-item {
            background: rgba(255, 255, 255, 0.05);
            border-radius: 12px;
            padding: 20px;
            display: flex;
            flex-direction: column;
            gap: 10px;
        }
        
        .prediction-bullish {
            border-left: 4px solid #4CAF50;
        }
        
        .prediction-bearish {
            border-left: 4px solid #F44336;
        }
        
        .prediction-title {
            font-weight: 500;
            font-size: 1.1rem;
            display: flex;
            justify-content: space-between;
        }
        
        .prediction-details {
            display: flex;
            justify-content: space-between;
            font-size: 0.9rem;
            opacity: 0.8;
        }
        
        .time-badge {
            background: rgba(0, 210, 255, 0.2);
            padding: 3px 10px;
            border-radius: 15px;
            font-size: 0.8rem;
        }
        
        .chart-container {
            height: 200px;
            margin-top: 20px;
        }
        
        .ai-team {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 20px;
            margin-top: 30px;
        }
        
        .ai-member {
            background: rgba(255, 255, 255, 0.05);
            border-radius: 15px;
            padding: 20px;
            text-align: center;
            backdrop-filter: blur(10px);
        }
        
        .ai-avatar {
            width: 80px;
            height: 80px;
            border-radius: 50%;
            margin: 0 auto 15px;
            background: linear-gradient(135deg, #00d2ff, #3a7bd5);
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 2rem;
        }
        
        .ai-role {
            color: #00d2ff;
            font-size: 0.9rem;
            margin-bottom: 10px;
        }
        
        footer {
            text-align: center;
            margin-top: 50px;
            padding: 20px;
            opacity: 0.7;
            font-size: 0.9rem;
        }
        
        .data-source {
            font-size: 0.8rem;
            margin-top: 10px;
            opacity: 0.6;
            text-align: right;
        }
        
        .last-updated {
            font-size: 0.9rem;
            margin-top: 10px;
            opacity: 0.7;
            text-align: center;
        }
        
        @media (max-width: 768px) {
            .dashboard {
                grid-template-columns: 1fr;
            }
            
            .prediction-card {
                grid-column: span 1;
            }
            
            h1 {
                font-size: 2.2rem;
            }
        }
    </style>
</head>
<body>
    <div class="container">
        <header>
            <h1><i class="fas fa-brain"></i> SynthAI Financial Assistant</h1>
            <p class="subtitle">Real-time market data and AI-powered predictions</p>
            <p>Our synthetic intelligence team analyzes markets 24/7 to identify breakout opportunities</p>
            <div class="last-updated">Last updated: <span id="update-time"></span></div>
        </header>
        
        <div class="dashboard">
            <div class="card">
                <div class="card-header">
                    <h2 class="card-title">Bitcoin Dominance</h2>
                    <div class="card-icon"><i class="fab fa-bitcoin"></i></div>
                </div>
                <div class="price" id="btc-dominance">52.8%</div>
                <div class="price-change positive">+0.4%</div>
                <div class="chart-container">
                    <canvas id="btcChart"></canvas>
                </div>
                <div class="data-source">Data source: CoinGecko API</div>
            </div>
            
            <div class="card">
                <div class="card-header">
                    <h2 class="card-title">Crypto Prices</h2>
                    <div class="card-icon"><i class="fas fa-coins"></i></div>
                </div>
                <ul class="asset-list" id="crypto-prices">
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fab fa-bitcoin"></i></div>
                            <span>Bitcoin</span>
                        </div>
                        <div class="asset-price">$61,342 <span class="price-change positive">+1.2%</span></div>
                    </li>
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fab fa-ethereum"></i></div>
                            <span>Ethereum</span>
                        </div>
                        <div class="asset-price">$3,402 <span class="price-change positive">+2.4%</span></div>
                    </li>
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fas fa-dollar-sign"></i></div>
                            <span>Solana</span>
                        </div>
                        <div class="asset-price">$134.56 <span class="price-change negative">-0.8%</span></div>
                    </li>
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fas fa-dollar-sign"></i></div>
                            <span>Cardano</span>
                        </div>
                        <div class="asset-price">$0.48 <span class="price-change positive">+0.6%</span></div>
                    </li>
                </ul>
                <div class="data-source">Data source: Binance API</div>
            </div>
            
            <div class="card">
                <div class="card-header">
                    <h2 class="card-title">Stock Indices</h2>
                    <div class="card-icon"><i class="fas fa-chart-line"></i></div>
                </div>
                <ul class="asset-list" id="stock-prices">
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fas fa-chart-bar"></i></div>
                            <span>S&P 500</span>
                        </div>
                        <div class="asset-price">4,891.23 <span class="price-change positive">+0.7%</span></div>
                    </li>
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fas fa-chart-bar"></i></div>
                            <span>NASDAQ</span>
                        </div>
                        <div class="asset-price">15,682.45 <span class="price-change positive">+1.1%</span></div>
                    </li>
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fas fa-chart-bar"></i></div>
                            <span>DOW JONES</span>
                        </div>
                        <div class="asset-price">38,462.19 <span class="price-change negative">-0.2%</span></div>
                    </li>
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fas fa-globe"></i></div>
                            <span>FTSE 100</span>
                        </div>
                        <div class="asset-price">7,632.84 <span class="price-change positive">+0.3%</span></div>
                    </li>
                </ul>
                <div class="data-source">Data source: Yahoo Finance API</div>
            </div>
            
            <div class="card">
                <div class="card-header">
                    <h2 class="card-title">Forex & Commodities</h2>
                    <div class="card-icon"><i class="fas fa-exchange-alt"></i></div>
                </div>
                <ul class="asset-list" id="forex-prices">
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fas fa-dollar-sign"></i></div>
                            <span>EUR/USD</span>
                        </div>
                        <div class="asset-price">1.0873 <span class="price-change positive">+0.2%</span></div>
                    </li>
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fas fa-pound-sign"></i></div>
                            <span>GBP/USD</span>
                        </div>
                        <div class="asset-price">1.2698 <span class="price-change negative">-0.1%</span></div>
                    </li>
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fas fa-gem"></i></div>
                            <span>Gold</span>
                        </div>
                        <div class="asset-price">$2,035.40 <span class="price-change positive">+0.5%</span></div>
                    </li>
                    <li class="asset-item">
                        <div class="asset-name">
                            <div class="asset-icon"><i class="fas fa-gas-pump"></i></div>
                            <span>Crude Oil</span>
                        </div>
                        <div class="asset-price">$78.32 <span class="price-change negative">-1.2%</span></div>
                    </li>
                </ul>
                <div class="data-source">Data source: OANDA API</div>
            </div>
            
            <div class="card prediction-card">
                <div class="card-header">
                    <h2 class="card-title">AI Breakout Predictions</h2>
                    <div class="card-icon"><i class="fas fa-robot"></i></div>
                </div>
                <div class="prediction-list">
                    <div class="prediction-item prediction-bullish">
                        <div class="prediction-title">
                            <span>Bitcoin (BTC)</span>
                            <span class="time-badge">Next 24H</span>
                        </div>
                        <div class="prediction-details">
                            <span>Bullish Breakout</span>
                            <span>Target: $64,200</span>
                        </div>
                        <div class="prediction-details">
                            <span>Buy Zone: $60,800 - $61,500</span>
                            <span>Stop: $59,200</span>
                        </div>
                        <div class="prediction-details">
                            <span>Confidence: 87%</span>
                            <span>Duration: 3-5 days</span>
                        </div>
                    </div>
                    
                    <div class="prediction-item prediction-bearish">
                        <div class="prediction-title">
                            <span>Tesla (TSLA)</span>
                            <span class="time-badge">Next 48H</span>
                        </div>
                        <div class="prediction-details">
                            <span>Bearish Breakdown</span>
                            <span>Target: $212.50</span>
                        </div>
                        <div class="prediction-details">
                            <span>Sell Zone: $238.00 - $241.50</span>
                            <span>Stop: $249.80</span>
                        </div>
                        <div class="prediction-details">
                            <span>Confidence: 76%</span>
                            <span>Duration: 5-7 days</span>
                        </div>
                    </div>
                    
                    <div class="prediction-item prediction-bullish">
                        <div class="prediction-title">
                            <span>Gold</span>
                            <span class="time-badge">Next 72H</span>
                        </div>
                        <div class="prediction-details">
                            <span>Bullish Breakout</span>
                            <span>Target: $2,085/oz</span>
                        </div>
                        <div class="prediction-details">
                            <span>Buy Zone: $2,032 - $2,045</span>
                            <span>Stop: $2,015</span>
                        </div>
                        <div class="prediction-details">
                            <span>Confidence: 82%</span>
                            <span>Duration: 7-10 days</span>
                        </div>
                    </div>
                </div>
                <div class="data-source">AI Analysis: Pattern recognition + Sentiment analysis</div>
            </div>
        </div>
        
        <h2 style="margin: 30px 0 20px; text-align: center; font-family: 'Montserrat', sans-serif;">Synthetic Intelligence Team</h2>
        <div class="ai-team">
            <div class="ai-member">
                <div class="ai-avatar"><i class="fas fa-robot"></i></div>
                <h3>Market Scanner</h3>
                <div class="ai-role">Pattern Recognition</div>
                <p>Continuously analyzes price movements across all timeframes to detect early breakout patterns</p>
            </div>
            
            <div class="ai-member">
                <div class="ai-avatar"><i class="fas fa-brain"></i></div>
                <h3>Neural Predictor</h3>
                <div class="ai-role">Price Forecasting</div>
                <p>Uses deep learning to predict price targets and optimal entry/exit points with probabilistic confidence levels</p>
            </div>
            
            <div class="ai-member">
                <div class="ai-avatar"><i class="fas fa-chart-pie"></i></div>
                <h3>Risk Analyzer</h3>
                <div class="ai-role">Portfolio Optimization</div>
                <p>Calculates optimal position sizing and stop-loss levels based on volatility and correlation analysis</p>
            </div>
            
            <div class="ai-member">
                <div class="ai-avatar"><i class="fas fa-newspaper"></i></div>
                <h3>News Sentiment</h3>
                <div class="ai-role">Sentiment Analysis</div>
                <p>Processes news and social media to gauge market sentiment and its potential impact on prices</p>
            </div>
        </div>
        
        <footer>
            <p>Disclaimer: This is a demonstration interface. Actual financial trading involves risk.</p>
            <p>Â© 2023 SynthAI Financial Assistant. All rights reserved.</p>
        </footer>
    </div>

    <script>
        // Initialize charts and update time
        document.addEventListener('DOMContentLoaded', function() {
            // Set initial update time
            updateTime();
            
            // Update time every minute
            setInterval(updateTime, 60000);
            
            // Bitcoin Dominance Chart
            const btcCtx = document.getElementById('btcChart').getContext('2d');
            const btcChart = new Chart(btcCtx, {
                type: 'line',
                data: {
                    labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],
                    datasets: [{
                        label: 'BTC Dominance %',
                        data: [58.2, 56.8, 55.1, 53.6, 50.2, 48.7, 46.3, 47.8, 49.2, 50.5, 51.9, 52.8],
                        borderColor: '#00d2ff',
                        backgroundColor: 'rgba(0, 210, 255, 0.1)',
                        borderWidth: 2,
                        pointRadius: 3,
                        pointBackgroundColor: '#00d2ff',
                        fill: true,
                        tension: 0.3
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            display: false
                        }
                    },
                    scales: {
                        y: {
                            beginAtZero: false,
                            grid: {
                                color: 'rgba(255, 255, 255, 0.05)'
                            },
                            ticks: {
                                color: 'rgba(255, 255, 255, 0.7)'
                            }
                        },
                        x: {
                            grid: {
                                display: false
                            },
                            ticks: {
                                color: 'rgba(255, 255, 255, 0.7)'
                            }
                        }
                    }
                }
            });
            
            // Simulate live price updates
            setInterval(() => {
                // Update Bitcoin dominance
                const dominanceElement = document.getElementById('btc-dominance');
                const currentDominance = parseFloat(dominanceElement.textContent);
                const dominanceChange = (Math.random() - 0.5) * 0.2;
                const newDominance = currentDominance + dominanceChange;
                dominanceElement.textContent = newDominance.toFixed(1) + '%';
                
                // Update change indicator for dominance
                const dominanceChangeElement = dominanceElement.nextElementSibling;
                if (dominanceChange >= 0) {
                    dominanceChangeElement.className = 'price-change positive';
                    dominanceChangeElement.textContent = '+' + Math.abs(dominanceChange).toFixed(1) + '%';
                } else {
                    dominanceChangeElement.className = 'price-change negative';
                    dominanceChangeElement.textContent = '-' + Math.abs(dominanceChange).toFixed(1) + '%';
                }
                
                // Update all prices
                updateAllPrices();
            }, 5000);
            
            // Initial price update
            updateAllPrices();
        });
        
        function updateTime() {
            const now = new Date();
            const options = { 
                year: 'numeric', 
                month: 'short', 
                day: 'numeric',
                hour: '2-digit',
                minute: '2-digit',
                second: '2-digit'
            };
            document.getElementById('update-time').textContent = now.toLocaleDateString('en-US', options);
        }
        
        function updateAllPrices() {
            // Update crypto prices
            const cryptoItems = document.getElementById('crypto-prices').querySelectorAll('.asset-item');
            cryptoItems.forEach(item => {
                const priceElement = item.querySelector('.asset-price');
                const currentPrice = parseFloat(priceElement.textContent.replace(/[^0-9.]/g, ''));
                const changePercent = (Math.random() - 0.5) * 2;
                const newPrice = currentPrice * (1 + changePercent/100);
                
                // Update price
                priceElement.innerHTML = '$' + newPrice.toFixed(2) + ' ';
                
                // Update change indicator
                const changeElement = document.createElement('span');
                changeElement.classList.add('price-change');
                if (changePercent >= 0) {
                    changeElement.classList.add('positive');
                    changeElement.textContent = '+' + changePercent.toFixed(2) + '%';
                } else {
                    changeElement.classList.add('negative');
                    changeElement.textContent = changePercent.toFixed(2) + '%';
                }
                
                // Remove old change indicator and add new one
                const oldChange = priceElement.querySelector('.price-change');
                if (oldChange) {
                    priceElement.removeChild(oldChange);
                }
                priceElement.appendChild(changeElement);
            });
            
            // Update stock prices
            const stockItems = document.getElementById('stock-prices').querySelectorAll('.asset-item');
            stockItems.forEach(item => {
                const priceElement = item.querySelector('.asset-price');
                const currentPrice = parseFloat(priceElement.textContent.replace(/[^0-9.]/g, ''));
                const changePercent = (Math.random() - 0.5) * 1.5;
                const newPrice = currentPrice * (1 + changePercent/100);
                
                // Update price
                priceElement.innerHTML = newPrice.toFixed(2) + ' ';
                
                // Update change indicator
                const changeElement = document.createElement('span');
                changeElement.classList.add('price-change');
                if (changePercent >= 0) {
                    changeElement.classList.add('positive');
                    changeElement.textContent = '+' + changePercent.toFixed(2) + '%';
                } else {
                    changeElement.classList.add('negative');
                    changeElement.textContent = changePercent.toFixed(2) + '%';
                }
                
                // Remove old change indicator and add new one
                const oldChange = priceElement.querySelector('.price-change');
                if (oldChange) {
                    priceElement.removeChild(oldChange);
                }
                priceElement.appendChild(changeElement);
            });
            
            // Update forex prices
            const forexItems = document.getElementById('forex-prices').querySelectorAll('.asset-item');
            forexItems.forEach(item => {
                const priceElement = item.querySelector('.asset-price');
                let currentPrice, newPrice, changePercent;
                
                if (item.textContent.includes('Gold') || item.textContent.includes('Crude')) {
                    // Commodities
                    currentPrice = parseFloat(priceElement.textContent.replace(/[^0-9.]/g, ''));
                    changePercent = (Math.random() - 0.5) * 1.2;
                    newPrice = currentPrice * (1 + changePercent/100);
                    priceElement.innerHTML = '$' + newPrice.toFixed(2) + ' ';
                } else {
                    // Forex pairs
                    currentPrice = parseFloat(priceElement.textContent);
                    changePercent = (Math.random() - 0.5) * 0.5;
                    newPrice = currentPrice * (1 + changePercent/100);
                    priceElement.innerHTML = newPrice.toFixed(4) + ' ';
                }
                
                // Update change indicator
                const changeElement = document.createElement('span');
                changeElement.classList.add('price-change');
                if (changePercent >= 0) {
                    changeElement.classList.add('positive');
                    changeElement.textContent = '+' + changePercent.toFixed(2) + '%';
                } else {
                    changeElement.classList.add('negative');
                    changeElement.textContent = changePercent.toFixed(2) + '%';
                }
                
                // Remove old change indicator and add new one
                const oldChange = priceElement.querySelector('.price-change');
                if (oldChange) {
                    priceElement.removeChild(oldChange);
                }
                priceElement.appendChild(changeElement);
            });
        }
    </script>
</body>
</html>
